{
  "rangeStrategy": "pin",
  "constraints": {
    "go": "1.22"
  },
  "packageRules": [
    {
      "matchPackageNames": [
        "k8s.io/api",
        "k8s.io/apiextensions-apiserver",
        "k8s.io/apimachinery",
        "k8s.io/client-go"
      ],
      "allowedVersions": "<=v0.31"
    },
    {
      "matchPackageNames": [
        "sigs.k8s.io/controller-runtime"
      ],
      "allowedVersions": "<=v0.19.3"
    }
  ]
}
